pragma solidity 0.6.12;

// import "./@pancakeswap/core/interfaces/IPancakePair.sol";

contract BOREv1Router is OwnableUpgradeSafe {
    using SafeMath for uint256;
    mapping(address => uint256) public hardBORE;

    address public _boreToken;
    address public _boreWETHPair;
    IFeeApprover public _feeApprover;
    IBoreVault public _boreVault;
    IWETH public _WETH;
    address public _uniV2Factory;

    function initialize(
        address boreToken,
        address WETH,
        address uniV2Factory,
        address boreWethPair,
        address feeApprover,
        address boreVault
    ) public initializer {
        OwnableUpgradeSafe.__Ownable_init();
        _boreToken = boreToken;
        _WETH = IWETH(WETH);
        _uniV2Factory = uniV2Factory;
        _feeApprover = IFeeApprover(feeApprover);
        _boreWETHPair = boreWethPair;
        _boreVault = IBoreVault(boreVault);
        refreshApproval();
    }

    function refreshApproval() public {
        IPancakePair(_boreWETHPair).approve(address(_boreVault), uint256(-1));
    }

    event FeeApproverChanged(
        address indexed newAddress,
        address indexed oldAddress
    );

    fallback() external payable {
        if (msg.sender != address(_WETH)) {
            addLiquidityETHOnly(msg.sender, false);
        }
    }

    function addLiquidityETHOnly(address payable to, bool autoStake)
        public
        payable
    {
        require(to != address(0), "Invalid address");
        hardBORE[msg.sender] = hardBORE[msg.sender].add(msg.value);

        uint256 buyAmount = msg.value.div(2);
        require(buyAmount > 0, "Insufficient ETH amount");
        _WETH.deposit{value: msg.value}();

        (uint256 reserveWeth, uint256 reserveBore) = getPairReserves();
        uint256 outBore =
            PancakeLibrary.getAmountOut(buyAmount, reserveWeth, reserveBore);

        _WETH.transfer(_boreWETHPair, buyAmount);

        (address token0, address token1) =
            PancakeLibrary.sortTokens(address(_WETH), _boreToken);
        IPancakePair(_boreWETHPair).swap(
            _boreToken == token0 ? outBore : 0,
            _boreToken == token1 ? outBore : 0,
            address(this),
            ""
        );

        _addLiquidity(outBore, buyAmount, to, autoStake);

        _feeApprover.sync();
    }

    function _addLiquidity(
        uint256 boreAmount,
        uint256 wethAmount,
        address payable to,
        bool autoStake
    ) internal {
        (uint256 wethReserve, uint256 boreReserve) = getPairReserves();

        uint256 optimalBoreAmount =
            PancakeLibrary.quote(wethAmount, wethReserve, boreReserve);

        uint256 optimalWETHAmount;
        if (optimalBoreAmount > boreAmount) {
            optimalWETHAmount = PancakeLibrary.quote(
                boreAmount,
                boreReserve,
                wethReserve
            );
            optimalBoreAmount = boreAmount;
        } else optimalWETHAmount = wethAmount;

        assert(_WETH.transfer(_boreWETHPair, optimalWETHAmount));
        assert(IERC20(_boreToken).transfer(_boreWETHPair, optimalBoreAmount));

        if (autoStake) {
            IPancakePair(_boreWETHPair).mint(address(this));
            _boreVault.depositFor(
                to,
                0,
                IPancakePair(_boreWETHPair).balanceOf(address(this))
            );
        } else IPancakePair(_boreWETHPair).mint(to);

        //refund dust
        if (boreAmount > optimalBoreAmount)
            IERC20(_boreToken).transfer(to, boreAmount.sub(optimalBoreAmount));

        if (wethAmount > optimalWETHAmount) {
            uint256 withdrawAmount = wethAmount.sub(optimalWETHAmount);
            _WETH.withdraw(withdrawAmount);
            to.transfer(withdrawAmount);
        }
    }

    function changeFeeApprover(address feeApprover) external onlyOwner {
        address oldAddress = address(_feeApprover);
        _feeApprover = IFeeApprover(feeApprover);

        emit FeeApproverChanged(feeApprover, oldAddress);
    }

    function getLPTokenPerEthUnit(uint256 ethAmt)
        public
        view
        returns (uint256 liquidity)
    {
        (uint256 reserveWeth, uint256 reserveBore) = getPairReserves();
        uint256 outBore =
            PancakeLibrary.getAmountOut(
                ethAmt.div(2),
                reserveWeth,
                reserveBore
            );
        uint256 _totalSupply = IPancakePair(_boreWETHPair).totalSupply();

        (address token0, ) =
            PancakeLibrary.sortTokens(address(_WETH), _boreToken);
        (uint256 amount0, uint256 amount1) =
            token0 == _boreToken
                ? (outBore, ethAmt.div(2))
                : (ethAmt.div(2), outBore);
        (uint256 _reserve0, uint256 _reserve1) =
            token0 == _boreToken
                ? (reserveBore, reserveWeth)
                : (reserveWeth, reserveBore);
        liquidity = Math.min(
            amount0.mul(_totalSupply) / _reserve0,
            amount1.mul(_totalSupply) / _reserve1
        );
    }

    function getPairReserves()
        internal
        view
        returns (uint256 wethReserves, uint256 boreReserves)
    {
        (address token0, ) =
            PancakeLibrary.sortTokens(address(_WETH), _boreToken);
        (uint256 reserve0, uint256 reserve1, ) =
            IPancakePair(_boreWETHPair).getReserves();
        (wethReserves, boreReserves) = token0 == _boreToken
            ? (reserve1, reserve0)
            : (reserve0, reserve1);
    }
}
